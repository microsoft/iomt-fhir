SELECT 
    DeviceId [DeviceId], 
    PatientId [PatientId],
    EncounterId [EncounterId],
    CorrelationId [CorrelationId],
    collect() [Data],
    System.Timestamp [WindowTime],
    Type [MeasureType],
    count(*) [Count]
INTO
    [FhirImportOutput]
FROM
    [NormalizedDeviceInput] PARTITION BY PartitionId TIMESTAMP BY OccurrenceTimeUtc
GROUP BY PartitionId,
    DeviceId, 
    PatientId, 
    EncounterId,
    CorrelationId,
    Type, 
    TUMBLINGWINDOW(MINUTE, 5)